<h1>Tarifas list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Tarifa</th>
            <th>Limitemenor</th>
            <th>Limitemayor</th>
            <th>Descuento</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('tarifas_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.tarifa }}</td>
            <td>{{ entity.limitemenor }}</td>
            <td>{{ entity.limitemayor }}</td>
            <td>{{ entity.descuento }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('tarifas_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('tarifas_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('tarifas_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
